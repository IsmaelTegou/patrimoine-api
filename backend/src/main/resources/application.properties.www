spring.application.name=patrimoine

spring.datasource.driver-class-name=org.postgresql.Driver
spring.datasource.url=jdbc:postgresql://localhost:5433/patrimoine_db
spring.datasource.username=ismael
spring.datasource.password=${SPRING_DATASOURCE_PASSWORD:ismael}
spring.jpa.show-sql=true
spring.jpa.properties.hibernate.format_sql=false
spring.jpa.database=postgresql
spring.jpa.hibernate.ddl-auto=update
springdoc.swagger-ui.tagsSorter=alpha

server.port=${SERVER_PORT:5014}